{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.5.6",
  "private": true,
  "scripts": {
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "npx next dev -p 3211",
    "build": "npx next build && node ./scripts/generate-sitemap",
    "serve": "npx next start",
    "analyze": "cross-env ANALYZE=true npx next build",
    "lint": "npx eslint '**/*.{js,jsx}'",
    "prettier": "npx prettier --write '**/*.{json,html,css,js,jsx,md,mdx}'",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.12",
    "@mailchimp/mailchimp_marketing": "^3.0.78",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.8",
    "clsx": "^1.2.1",
    "esbuild": "^0.14.54",
    "fs-jetpack": "^4.3.1",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.2",
    "mdx-bundler": "^9.0.1",
    "next": "12.3.0",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.16",
    "preact": "^10.10.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^0.4.0",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.0.1",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.31.0",
    "tailwindcss": "^3.1.8",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.3.0",
    "@svgr/webpack": "^6.3.1",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.23.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "globby": "13.1.2",
    "husky": "^8.0.1",
    "inquirer": "^9.1.1",
    "lint-staged": "^13.0.3",
    "next-remote-watch": "^1.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --cache --quiet --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
